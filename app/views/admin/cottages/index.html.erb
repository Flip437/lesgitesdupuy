<p id="notice"><%= notice %></p>

<h1>Admin Cottages</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_cottages.each do |admin_cottage| %>
      <tr>
        <td><%=admin_cottage.name%> </td>
        <td><%= link_to 'Show', admin_cottage %></td>
        <td><%= link_to 'Edit', edit_admin_cottage_path(admin_cottage) %></td>
        <td><%= link_to 'Destroy', admin_cottage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Cottage', new_admin_cottage_path %>
